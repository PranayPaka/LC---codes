class Solution {
    public List<String> buildArray(int[] target, int n) {
        List<String> list = new ArrayList<>();   
        int j = 0;
        for(int i = 1; i <= n + 1; i++){

            if(j == target.length){
               break;
            }       
            if( i == target[j]){
                list.add("Push");
                j += 1;
            }

            else{
            list.add("Push");
            list.add("Pop");
        }
        }

        return list;
    }
}
